{
  "id": "http://swagger-api.github.io/schemas/v1.1/dataType.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "dataType": {
      "type": "string"
    },
    "allowableValues": {
      "$ref": "#/definitions/allowableValues"
    }
  },
  "dependencies": {
    "allowableValues": [
      "dataType"
    ]
  },
  "definitions": {
    "allowableValues": {
      "type": "object",
      "oneOf": [
        {
          "properties": {
            "valueType": {
              "type": "string",
              "pattern": "^range\\[[0-9]+\\s*,\\s*[0-9]+\\]"
            }
          }
        },
        {
          "required": [
            "values"
          ],
          "properties": {
            "valueType": {
              "enum": [
                "LIST"
              ]
            },
            "values": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "minItems": 1,
              "uniqueItems": true
            }
          }
        }
      ]
    }
  }
}